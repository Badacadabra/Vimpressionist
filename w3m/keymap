# -----------------------------
# In-page Navigation
# -----------------------------

# Move to next page
keymap C-f NEXT_PAGE

# Move to previous page
keymap C-b PREV_PAGE

# Move cursor right (a half screen shift at the right edge)
keymap L MOVE_RIGHT

# Move cursor left (a half screen shift at the left edge)
keymap H MOVE_LEFT

# Move cursor down (a half screen scroll at the end of screen)
keymap J MOVE_DOWN

# Move cursor up (a half screen scroll at the top of screen)
keymap K MOVE_UP

# Move cursor right (1 columns shift at the right edge)
keymap l MOVE_RIGHT1

# Move cursor left (1 columns shift at the left edge)
keymap h MOVE_LEFT1

# Move cursor down (1 line scroll at the end of screen)
keymap j MOVE_DOWN1

# Move cursor up (1 line scroll at the top of screen)
keymap k MOVE_UP1

# Scroll down one line
keymap C-y DOWN

# Scroll up one line
keymap C-e UP

# Shift screen left
keymap <% SHIFT_LEFT

# Shift screen right
keymap >% SHIFT_RIGHT

# Shift screen one column left
keymap << LEFT

# Shift screen one column right
keymap >> RIGHT

# Go to the beginning of line
keymap 0 LINE_BEGIN

# Go to the end of line
keymap $ LINE_END

# Move to the center line
keymap M CENTER_H

# Move to the center column
keymap _ CENTER_V

# Go to specified line
# keymap GOTO_LINE

# Go to the first line
keymap gg BEGIN

# Go to the last line
keymap G END

# Move to next word
keymap w NEXT_WORD

# Move to previous word
keymap b PREV_WORD

# Go to the first link
keymap g0 LINK_BEGIN

# Go to the last link
keymap g$ LINK_END

# Move to next link
keymap C-n NEXT_LINK

# Move to previous link
keymap C-p PREV_LINK

# Popup link list menu and move cursor to selected link
keymap ESC-m MOVE_LIST_MENU

# Move to next right link
keymap ) NEXT_RIGHT

# Move to next left link
keymap ( NEXT_LEFT

# Move to next downward link
keymap } NEXT_DOWN

# Move to next upward link
keymap { NEXT_UP

# Move to next right (or downward) link
keymap ]] NEXT_RIGHT_DOWN

# Move to next left (or upward) link
keymap [[ NEXT_LEFT_UP

# Cancel the last cursor movement
keymap u UNDO

# Cancel the last undo
keymap C-r REDO

# -----------------------------
# Hyperlink Operations
# -----------------------------

# Go to current link
keymap f GOTO_LINK

# Open current link on new tab
keymap T TAB_LINK

# Save link to file
keymap W SAVE_LINK

# View image
keymap i VIEW_IMAGE

# Save image to file
keymap :W SAVE_IMAGE

# Submit form
keymap s SUBMIT

# Peek current URL
keymap p PEEK

# Peek link URL
keymap P PEEK_LINK

# Peek image URL
keymap I PEEK_IMG

# View info of current document
keymap :i INFO

# Show current line number
keymap C-g LINE_INFO

# Mark URL-like strings as anchors
keymap mu MARK_URL

# Mark current word as anchor
keymap mw MARK_WORD

# Mark Message-ID-like strings as anchors
keymap mm MARK_MID

# Render frame
keymap F FRAME

# Execute external browser
keymap ESC-B EXTERN

# View current link using external browser
keymap B EXTERN_LINK

# Show all links and images
keymap :L LIST

# Popup link list menu and go to selected link
keymap ESC-l LIST_MENU

# Popup acceskey menu
keymap ESC-a ACCESSKEY

# -----------------------------
# File/Stream Operations
# -----------------------------

# Go to URL
keymap :o GOTO
keymap gh GOTO https://duckduckgo.com/lite/

# Go to relative URL
keymap :O GOTO_RELATIVE

# Open URL on new tab
keymap t TAB_GOTO

# Open relative URL on new tab
keymap C-t TAB_GOTO_RELATIVE

# Load local file
keymap :r LOAD

# Execute shell command and load
keymap !r READ_SHELL

# Execute shell command and browse
keymap !| PIPE_SHELL

# Send rendered document to pipe
keymap | PIPE_BUF

# -----------------------------
# Buffer Operations
# -----------------------------

# Back to previous buffer
# keymap BACK

# Move to next buffer
keymap C-i NEXT

# Move to previous buffer
keymap C-o PREV

# Popup buffer selection menu
keymap ESC-b SELECT_MENU

# Go to buffer selection panel
keymap :ls SELECT

# View HTML source
keymap :v SOURCE

# Save document source to file
keymap :w SAVE

# Save buffer to file
keymap :p PRINT

# Edit current document
keymap :e EDIT

# Edit currently rendered document
keymap :E EDIT_SCREEN

# Reload buffer
keymap r RELOAD

# Re-render buffer
keymap R RESHAPE

# Redraw screen
keymap C-l REDRAW

# Restart loading and drawing of images
keymap ESC-i DISPLAY_IMAGE

# Stop loading and drawing of images
keymap X STOP_IMAGE

# -----------------------------
# Tab Operations
# -----------------------------

# Open new tab
keymap :t NEW_TAB

# Close current tab
keymap d CLOSE_TAB

# Close current tab
keymap x CLOSE_TAB

# Move to next tab
keymap gt NEXT_TAB

# Move to previous tab
keymap gT PREV_TAB

# Popup tab selection menu
keymap ESC-t TAB_MENU

# Move current tab right
keymap C-wl TAB_RIGHT

# Move current tab left
keymap C-wh TAB_LEFT

# -----------------------------
# Bookmark Management
# -----------------------------

# Read bookmark
keymap :b BOOKMARK

# Add current page to bookmark
keymap a ADD_BOOKMARK

# -----------------------------
# Searches
# -----------------------------

# Search forward
keymap ESC-/ WHEREIS

# Search backward
keymap ESC-? SEARCH_BACK

# Search next regexp
keymap n SEARCH_NEXT

# Search previous regexp
keymap N SEARCH_PREV

# Incremental search forward
keymap / ISEARCH

# Incremental search backward
keymap ? ISEARCH_BACK

# -----------------------------
# Dictionary Lookup
# -----------------------------

# Execute dictionary command (see README.dict)
keymap ESC-w DICT_WORD

# Execute dictionary command for word at cursor
keymap ESC-W DICT_WORD_AT

# -----------------------------
# Mark Operations
# -----------------------------

# Set/unset mark
keymap :m MARK

# Move to next mark
keymap \" NEXT_MARK

# Move to previous mark
keymap \' PREV_MARK

# Set mark using regexp
keymap C-m REG_MARK

# -----------------------------
# Miscellaneous
# -----------------------------

# Popup menu
keymap SPC MENU

# View help
keymap :h HELP

# Option setting panel
keymap ESC-o OPTIONS

# Display version of w3m
keymap :V VERSION

# View cookie list
keymap :c COOKIE

# View history of URL
keymap :H HISTORY

# Display download list panel
keymap :d DOWNLOAD_LIST

# Change the current document charset
keymap ESC-c CHARSET

# Change the default document charset
keymap ESC-C DEFAULT_CHARSET

# Display error messages
keymap ESC-M MSGS

# Toggle activity of mouse
# keymap MOUSE_TOGGLE

# Toggle wrap search mode
keymap C-h WRAP_TOGGLE

# Execute w3m command(s)
keymap :: COMMAND

# Set alarm
keymap ESC-A ALARM

# Set option
keymap ESC-O SET_OPTION

# Set environment variable
keymap ESC-E SETENV

# Define a binding between a key stroke and a user command
# keymap DEFINE_KEY

# Reload configuration files
keymap :R REINIT

# Execute shell command
keymap :! SHELL

# Stop loading document
keymap C-c SUSPEND

# Quit w3m
keymap :q QUIT

# Quit w3m without confirmation
keymap :x EXIT
keymap ZZ EXIT

# -----------------------------
# N/A
# -----------------------------

